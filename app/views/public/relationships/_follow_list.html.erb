<div class="container main">
 <div class="row toprow justify-content-center">

<% if end_users.exists? %>
  <% end_users.each do |end_user| %>
    <table class="table">
      <thead>
        <tr>
          <th>名前</th>
          <th>フォロー数</th>
          <th>フォロワー数</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><%=end_user.full_name %></td>
          <td><%= end_user.followings.count %></td>
          <td><%= end_user.followers.count %></td>
        </tr>
      </tbody>
    </table>
  <% end %>
<% else %>
  <p>ユーザーはいません</p>
<% end %>
</div>
</div>