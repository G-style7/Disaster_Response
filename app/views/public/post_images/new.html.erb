<h1>画像投稿フォーム</h1>
  <%= form_with model: @post_image do |f| %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>タイトル</h4>
      <%= f.text_field :title %>
    <h4>種別</h4>
      <%= f.text_field :kind %>
    <h4>説明</h4>
      <%= f.text_area :introduction %>
    <%= f.submit '投稿' %>
  <% end %>
