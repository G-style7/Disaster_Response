<h2>Results index</h2>

<table class="table table-hover table-inverse">
  <!--検索対象モデルがUserの時 -->
  <% if @range == "End_User" %>
    <tbody>
      <% @end_users.each do |end_user| %>
        <tr>
          <td><%= end_user.first_name %></td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <!--検索対象モデルがUserではない時(= 検索対象モデルがBookの時) -->
    <tbody>
      <% @post_images.each do |post_image| %>
        <tr>
          <td><%= post_image.title %></td>
          <td><%= post_image.kind %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>