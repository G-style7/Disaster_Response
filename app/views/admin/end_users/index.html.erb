<div class ="container">
   <div class ="row justify-content-center">
    <h3>会員一覧</h3>
   </div>
    <table class = "table mt-3">
     <div class "col">
      <thead class ="bg-light">
       <tr>
        <th>会員ID</th>
        <th>氏名</th>
        <th>メールアドレス</th>
        <th>ステータス</th>
       </tr>
      </thead>
      <tbody>
       <% @end_users.each do |end_user| %>
       <tr>
        <td><%= end_user.id %></td>
        <td><u><%= link_to end_user.full_name ,admin_end_user_path(end_user.id) %></u></td>
        <td><%= end_user.email %></td>
        <td>
         <% if end_user.is_deleted %>
          <p class ="text-secondary font-weight-bold">退会</p>
         <% else %>
          <p class ="text-success font-weight-bold">有効</p>
         <% end %>
        </td>
       </tr>
       <% end %>
      </tbody>
     </div>
    </table>
    <div class ="row">
     <div class ="mx-auto">
      <%= paginate @end_users %>
     </div>
    </div>
  </div>
